{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "plugins": ["spellcheck"],
    "rules": {
        "no-undef": ["error"],
        "spellcheck/spell-checker": ["warn"]
    }
}
